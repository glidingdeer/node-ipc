{
    "name": "@glidingdeer/node-ipc",
    "version": "10.0.0",
    "description": "A nodejs module for local and remote Inter Process Communication (IPC), Neural Networking, and able to facilitate machine learning.",
    "main": "node-ipc.js",
    "directories": {
        "example": "example"
    },
    "engines": {
        "node": ">=4.0.0"
    },
    "dependencies": {
        "event-pubsub": "4.3.0",
        "js-message": "1.0.5",
        "js-queue": "2.0.0"
    },
    "devDependencies": {
        "codacy-coverage": "2.0.0",
        "jasmine": "2.4.1",
        "istanbul": "0.4.1",
        "node-cmd": "2.0.0"
    },
    "scripts": {
        "test-windows": "istanbul cover -x **/spec/** -dir ./spec/coverage ./node_modules/jasmine/bin/jasmine.js",
        "test": "istanbul cover -x **/spec/** -dir ./spec/coverage jasmine",
        "cover": "istanbul cover -x **/spec/** -dir ./spec/coverage jasmine",
        "coverup": "cat ./spec/coverage/lcov.info | codacy-coverage"
    },
    "keywords": [],
    "author": "Brandon Nozaki Miller",
    "license": "DBAD",
    "repository": {
        "type": "git",
        "url": "https://github.com/glidingdeer/node-ipc.git"
    },
    "bugs": {
        "url": "https://github.com/glidingdeer/node-ipc/issues"
    },
    "homepage": "https://github.com/glidingdeer/node-ipc#readme"
}
